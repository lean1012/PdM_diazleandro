#include "sht4x.h"

int8_t sht4x_write(uint8_t address, const uint8_t* data, uint16_t data_length);

int8_t sht4x_read(uint8_t address, const uint8_t* data, uint16_t data_length);

int8_t sht4x_sleep();

int sht4x_read_serial_number(){
	uint8_t buffer[6];
	buffer[0] = SHT4X_SERIAL_NUMBER;
	sht4x_write_command(SHT4X_I2C_ADDRESS,buffer,1);


	if (rc < 0) {
		printk("Failed to start measurement.\n");
		return rc;
	}

	sht4x_sleep();

	sht4x_read_command(SHT4X_I2C_ADDRESS,buffer,1);

}



